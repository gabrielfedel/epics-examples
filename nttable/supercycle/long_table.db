# Test with NTtable to use phoebus table plugin

record(aai, "$(P)Titles") {
    field(FTVL, "STRING")
    field(NELM, "30")
    field(PINI,"YES")
    field(INP , {const:["Col0","Col1","Col2","Col3","Col4","Col5","Col6","Col7","Col8","Col9","Col10","Col11","Col12","Col13","Col14","Col15","Col16","Col17","Col18","Col19","Col20","Col21","Col22","Col23","Col24","Col25","Col26","Col27","Col28","Col29"]})
    info(Q:group, {
        "$(P)MyTable":{
            +id:"epics:nt/NTTable:1.0",
            "labels":{+type:"plain", +channel:"VAL", +putorder:1}
        }
    })
}

record(waveform, "$(P)Col0"){
    field(NELM, "$(SIZE=100)")
    field(FTVL, "LONG")
    field(INP , "[0,1,2]")
    info(Q:group, {
        "$(P)MyTable":{
            "value.Col0":{+type:"plain", +channel:"VAL", +putorder:2}
        }
    })
}


