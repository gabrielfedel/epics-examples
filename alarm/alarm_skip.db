    field(INPC, "$(PV).SEVR")
    field(CALC, "B=18&&C=3?0:1")
    field(OCAL, "A")
    field(OOPT, "When Non-Zero")
    field(DOPT, "Use OCAL")
    field(OUT,  "$(P)$(R=)$(PV)Copy")
}


