record(ao, "PVVALUE") {
    field(PREC,"12")
    field(FLNK, "PVTESTCOMP PP")
}

record(compress, "PVTESTCOMP") {
    field(INP, "PVVALUE")
    field(ALG, "Circular Buffer")
    field(NSAM, "$(NSAM)")
    field(N, "10")
    field(FLNK, "PVRESULT PP")
}


record(waveform, "PVRESULT"){
    field(INP, "PVTESTCOMP")
    field(NELM, "100")
    field(FTVL, "DOUBLE")
}


